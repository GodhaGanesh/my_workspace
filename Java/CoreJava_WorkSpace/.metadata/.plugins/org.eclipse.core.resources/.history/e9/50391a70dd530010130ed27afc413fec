package oops_programming_style7;

public class Author {
	protected static String[] authors = {"chethan barath","sudha murthy","anitha desai", "vikram seth"};
	private String name;
	private int age;
	private int booksCount;
	private
	
	public Author(String name,int authorAge, int authorBooksCount) {
		if (AuthorValidation.verifyName(name)&&AuthorValidation.verifyAge(authorAge)&&AuthorValidation.verifyBooksCount(authorBooksCount)) {
			this.name = name;
		}
	}
}